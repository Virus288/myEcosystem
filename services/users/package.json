{
  "name": "users",
  "version": "0.1.0",
  "description": "Users",
  "productName": "Users",
  "author": "https://github.com/Virus288",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "start": "cross-env NODE_ENV=prod node build/src/main.js",
    "dev": "cross-env NODE_ENV=dev nodemon build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "cross-env NODE_ENV=development eslint . --ext .ts",
    "listErrors": "node_modules/.bin/tsc --skipLibCheck",
    "lintStaged": "npx lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./jest.config.e2e.ts",
    "test:unit": "jest --config ./jest.config.unit.ts"
  },
  "lint-staged": {
    "*.ts": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.0",
    "chalk": "^4.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.3",
    "node": "^18.7.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/amqplib": "^0.10.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.4.1",
    "rimraf": "^3.0",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsutils": "^3.21.0",
    "typescript": "^4.7.0"
  }
}
